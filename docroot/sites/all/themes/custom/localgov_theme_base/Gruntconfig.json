{
  "name": "localgov_theme_base",
  "title": "LocalGov Base Theme",
  "drupalversion": "7",
  "repository": "git@gitlab.web-dev.bracknell-forest.gov.uk:drupal-themes/localgov_theme_base.git",
  "plDir": "pattern-lab/",
  "jsDir": "js/",
  "scssDir": "scss/",
  "scssDest": "css/localgov_theme_base.styles.css",
  "scssConfigRoot": "./",
  "serverDir": "./",
  "serverPath": "pattern-lab/public/",
  "updateAtStart": true,
  "buildModernizrAtStart": true,
  "openBrowserAtStart": true,
  "babel": {
    "es6": {
      "dest": "js/localgov_theme_base.scripts-es6.js"
    },
    "js": {
      "dest": "js/localgov_theme_base.scripts.js"
    }
  },
  "webfont": {
    "options": {
      "font": "localgov_theme_base-icons"
    }
  }
}
